{
  "dashboard": {
    "title": "Hyperliquid MM Bot - Quantization Observability",
    "tags": ["mm-bot", "quantization", "sre"],
    "timezone": "utc",
    "editable": true,
    "graphTooltip": 1,
    "panels": [
      {
        "id": 1,
        "title": "Tick Error Rate by Pair/Side (last 15m)",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0},
        "targets": [
          {
            "expr": "sum(rate({job=\"mm-bot\"} |= \"quant_evt=submit\" | logfmt | err=\"tick_size\" [5m])) by (pair, side) / sum(rate({job=\"mm-bot\"} |= \"quant_evt=submit\" | logfmt [5m])) by (pair, side)",
            "legendFormat": "{{pair}}_{{side}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 0.05, "color": "yellow"},
                {"value": 0.08, "color": "red"}
              ]
            }
          }
        },
        "options": {
          "legend": {"displayMode": "table", "placement": "right"},
          "tooltip": {"mode": "multi"}
        }
      },
      {
        "id": 2,
        "title": "Attemptâ†’Submit Latency (P50/P95) by Pair",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0},
        "targets": [
          {
            "expr": "quantile_over_time(0.5, {job=\"mm-bot\"} |= \"quant_evt=submit\" | logfmt | unwrap tms [5m]) by (pair) - quantile_over_time(0.5, {job=\"mm-bot\"} |= \"quant_evt=attempt\" | logfmt | unwrap tms [5m]) by (pair)",
            "legendFormat": "{{pair}} P50",
            "refId": "A"
          },
          {
            "expr": "quantile_over_time(0.95, {job=\"mm-bot\"} |= \"quant_evt=submit\" | logfmt | unwrap tms [5m]) by (pair) - quantile_over_time(0.95, {job=\"mm-bot\"} |= \"quant_evt=attempt\" | logfmt | unwrap tms [5m]) by (pair)",
            "legendFormat": "{{pair}} P95",
            "refId": "B"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "ms",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 250, "color": "yellow"},
                {"value": 500, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 3,
        "title": "Error Code Distribution (last 1h)",
        "type": "bargauge",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 8},
        "targets": [
          {
            "expr": "sum(count_over_time({job=\"mm-bot\"} |= \"err_code=\" | logfmt | err_code != \"\" [1h])) by (err_code)",
            "legendFormat": "{{err_code}}",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 10, "color": "yellow"},
                {"value": 50, "color": "red"}
              ]
            }
          }
        },
        "options": {
          "orientation": "horizontal",
          "displayMode": "gradient"
        }
      },
      {
        "id": 4,
        "title": "Suppression Events (SOL)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 8, "x": 8, "y": 8},
        "targets": [
          {
            "expr": "count_over_time({job=\"mm-bot\"} |= \"sol_suppressed_60s\" | regexp \"ðŸ”´.*entering\" [1h])",
            "legendFormat": "Suppression Activations",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 3, "color": "yellow"},
                {"value": 10, "color": "red"}
              ]
            }
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "none"
        }
      },
      {
        "id": 5,
        "title": "Spec Refresh Events (last 24h)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 8, "x": 8, "y": 12},
        "targets": [
          {
            "expr": "count_over_time({job=\"mm-bot\"} |= \"SOL specs changed\" [24h])",
            "legendFormat": "Spec Changes",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 5, "color": "yellow"},
                {"value": 20, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 6,
        "title": "Order Intent (TIF Distribution)",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 8, "x": 16, "y": 8},
        "targets": [
          {
            "expr": "sum(count_over_time({job=\"mm-bot\"} |= \"quant_evt=attempt\" | logfmt [1h])) by (tif)",
            "legendFormat": "{{tif}}",
            "refId": "A"
          }
        ],
        "options": {
          "legend": {"displayMode": "table", "placement": "right"},
          "pieType": "donut"
        }
      },
      {
        "id": 7,
        "title": "Success Rate (ok=1 vs ok=0)",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 16},
        "targets": [
          {
            "expr": "sum(rate({job=\"mm-bot\"} |= \"quant_evt=submit\" | logfmt | ok=\"1\" [5m])) / sum(rate({job=\"mm-bot\"} |= \"quant_evt=submit\" | logfmt [5m]))",
            "legendFormat": "Success Rate",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "percentunit",
            "min": 0,
            "max": 1,
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "red"},
                {"value": 0.9, "color": "yellow"},
                {"value": 0.95, "color": "green"}
              ]
            }
          }
        }
      },
      {
        "id": 8,
        "title": "Recent Quantization Events (live tail)",
        "type": "logs",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 16},
        "targets": [
          {
            "expr": "{job=\"mm-bot\"} |= \"quant_evt=\"",
            "refId": "A"
          }
        ],
        "options": {
          "showTime": true,
          "wrapLogMessage": false,
          "dedupStrategy": "none"
        }
      },
      {
        "id": 9,
        "title": "Below Min Notional Events (last 1h)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 8, "x": 16, "y": 0},
        "targets": [
          {
            "expr": "count_over_time({job=\"mm-bot\"} |= \"quant_evt=below_min\" [1h])",
            "legendFormat": "Below Min Notional",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short",
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"value": 0, "color": "green"},
                {"value": 10, "color": "yellow"},
                {"value": 50, "color": "red"}
              ]
            }
          }
        }
      },
      {
        "id": 10,
        "title": "Sequence Counter (monotonicity check)",
        "type": "timeseries",
        "gridPos": {"h": 6, "w": 12, "x": 0, "y": 24},
        "targets": [
          {
            "expr": "{job=\"mm-bot\"} |= \"quant_evt=attempt\" | logfmt | unwrap seq",
            "legendFormat": "seq",
            "refId": "A"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "unit": "short"
          }
        },
        "options": {
          "tooltip": {"mode": "single"}
        }
      },
      {
        "id": 11,
        "title": "Build Version Tracking",
        "type": "table",
        "gridPos": {"h": 6, "w": 12, "x": 12, "y": 24},
        "targets": [
          {
            "expr": "{job=\"mm-bot\"} |= \"ðŸ”§ Build=\" | regexp \"Build=(?P<build>.*)\"",
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {},
              "indexByName": {"build": 0, "Time": 1},
              "renameByName": {"build": "Build Hash", "Time": "First Seen"}
            }
          }
        ]
      }
    ],
    "refresh": "10s",
    "time": {"from": "now-1h", "to": "now"},
    "timepicker": {
      "refresh_intervals": ["5s", "10s", "30s", "1m", "5m"]
    }
  }
}
